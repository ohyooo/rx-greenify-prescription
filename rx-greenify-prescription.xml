<prescriptions xmlns="http://greenify.github.io/schemas/prescription/v3">
    <prescription type="activity" sender="other">
        <intent-filter>
            <action name="android.intent.action.VIEW" />
            <scheme name="market" />
            <ssp prefix="//details?id=com.taobao.taobao" />
        </intent-filter>
        <intent-filter>
            <action name="android.intent.action.VIEW" />
            <scheme name="http" />
            <ssp sglob="//market.android.com/.*com.taobao.taobao.*" />
        </intent-filter>
    </prescription>
