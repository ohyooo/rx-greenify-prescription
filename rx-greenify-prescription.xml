<prescriptions xmlns="http://greenify.github.io/schemas/prescription/v3">
    <prescription type="activity" sender="other">
        <intent-filter>
            <action name="android.intent.action.VIEW" />
            <scheme name="market" />
            <ssp prefix="//details?id=com.taobao" />
        </intent-filter>
        <intent-filter>
            <action name="android.intent.action.VIEW" />
            <scheme name="http" />
            <ssp sglob="//market.android.com/.*com.taobao.*" />
        </intent-filter>
    </prescription>
    <prescription type="activity" sender="other">
        <intent-filter>
            <action name="android.intent.action.VIEW" />
            <scheme name="market" />
            <ssp prefix="//details?id=com.tmall" />
        </intent-filter>
        <intent-filter>
            <action name="android.intent.action.VIEW" />
            <scheme name="http" />
            <ssp sglob="//market.android.com/.*com.tmall.*" />
        </intent-filter>
    </prescription>
    <prescription type="activity" sender="other" package="com.netease.cloudmusic">
        <intent-filter>
            <action name="android.intent.action.VIEW" />
            <category name="android.intent.category.BROWSABLE" />
            <scheme name="https" />
            <scheme name="http" />
        </intent-filter>
    </prescription>
</prescriptions>
